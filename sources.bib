  keywords   = {Computer Science - Social and Information Networks, 68R10, 05C82, Computer Science - Discrete Mathematics, G.2.2, G.2.3, Physics - Physics and Society},
  file       = {arXiv Fulltext PDF:/Users/luca/Zotero/storage/X2BTW7T3/Goswami et al. - 2016 - Sparsity Measure of a Network Graph Gini Index.pdf:application/pdf;arXiv.org Snapshot:/Users/luca/Zotero/storage/JZ6ZZP8Y/1612.html:text/html}
