  langid       = {english},
  keywords     = {Complexity, Graph, Network},
  file         = {ScienceDirect Full Text PDF:/Users/luca/Zotero/storage/NAEX3JQR/Kim and Wilhelm - 2008 - What is a complex graph.pdf:application/pdf;ScienceDirect Snapshot:/Users/luca/Zotero/storage/XKSZ9WS5/S0378437108000319.html:text/html}
